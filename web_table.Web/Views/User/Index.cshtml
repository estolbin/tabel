@model IEnumerable<web_tabel.Domain.AppUser>
@{
    Layout = "_Layout";
}

<a href="@Url.Action("Create")" class="btn btn-outline-primary">Добавить пользователя</a>
<br/>

<h2>Пользователи</h2>
<table class="table">
    <tr>
        <th>ID</th>
        <th>Имя</th>
        <th>Электронная почта</th>
        <th></th>
    </tr>
@foreach(var item in Model)
{
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Email</td>
        <td>
            <a href="@Url.Action("Edit", new {id = item.Id})">Редактировать</a>
        </td>
    </tr>
}
</table>